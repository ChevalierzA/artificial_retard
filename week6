void fun(int a,int b,int c){
	double temp = b*b-4*a*c;
	if(temp<0)cout<<"DNE\n";
	else{
	double x1=(-b+sqrt(temp))/2;
	double x2=(-b-sqrt(temp))/2;
	cout<<"x1= "<<x1<<endl;
	cout<<"x2= "<<x2<<endl;
	}

	}
float avg(int a,int b,int c){
float sum = a+b+c;
float avg=sum/3.0;
return avg;
}
void pts(){
	int r;
	do{
	cout<<"inter radius >= 5 "<<endl;
	cin>>r;
	}while(r<5);
	for(int i=r;i>=-r;i--){
		for(int j=-r;j<=r;j++){
		double d =sqrt((double)(i*i+j*j));
		if(d<=r) cout<<"*";
		else cout<<" "; 
		}
		cout<<endl;
	}

}
void cp(float a[3]){
	int i,j;
	for(i=0;i<3;i++){
		for(j=i;j<3;j++){
			if(a[j]<a[i])
			    flag=a[i];
				a[i]=a[j];
				a[j]=flag;
		}
	}
	for(int i=0;i<3;i++){
	cout<<a[i];
	}
}
bool isDigit(char a){
if(a<'9'&&a>'0') return true;
else return false;
}
