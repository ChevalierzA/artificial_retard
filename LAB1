//Q4
struct RationalNumber
{
	int a;
	int b;
};
double toDouble(RationalNumber a)
{
	double ans;
	ans = (a.a/(a.b*1.0));
	return ans;
}

//Q5
void standardize(RationalNumber *num){
    if (num->b<0) {
        num->a = 0-num->a;
        num->b = abs(num->b);
    }
}

void reduce(RationalNumber *num){
    int flag=1;
    int size=abs(num->a);
    if (abs(num->a)>abs(num->b)) {
        size = abs(num->b);
    }
    for (int i=1; i<=size; i++) {
        if (num->a%i==0&&num->b%i==0) {
            flag = i;
        }
        else continue;
    }
    num->a = num->a/flag;
    num->b = num->b/flag;
}

int gcd(int a,int b)
{
	a = abs(a);
	b = abs(b);
	if (b > a)
	{
		int temp = a;
		a = b;
		b = temp;
	}
	if(b==0)
		return a;
	else
		return gcd(a,a%b);
}

