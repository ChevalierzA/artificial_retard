void strings()
{
	StringStat a[6];
	for(int i = 0;i<5;i++)
	{
		cin>>a[i].s;
	}
	for(int i = 0;i<5;i++)
	{
		a[i].digit = 0;
		a[i].lower = 0;
		a[i].upper = 0;
		//caculatedigits(a[i]);
	}
	cout<<a[0].digit<<endl;
}

