#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
	int a=-1,temp1;
	int b=1;
	temp1=rand()%(b-a+1);
	cout<<temp1+a<<endl;
	return 0;
}

6.
#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
	int a=-1,a1=a*100+1;
	int b=1,b1=b*100-1;
	double z=rand()%(b1-a1);
	z=(z+a1)/100;
	cout<<z;
	return 0;
}

7.
#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
	int a,b,temp;
	cin>>a>>b;
	if(a>b){
	temp=a;
	a=b;
	b=temp;
	}
	temp=rand()%(b-a+1);
	cout<<temp+a<<endl;
	return 0;
}

8.
#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
	float a,b,temp;
	cin>>a>>b;
	if(a>b){
	temp=a;
	a=b;
	b=temp;
	}
	float z=rand()/(RAND_MAX+1);
	z=z*(b-a);
	z+=a;
	cout<<z;
	return 0;
}

