#include<iostream>
using namespace std;
int main(){
	char a;
	cout<<"input a digit\n";
	cin>>a;
	if('0'<=a&&a<='9') cout<<"The character you entered is a digit\n";
	else cout<<"The character you entered is not a digit\n";
	return 0;
}

10.
#include<iostream>
using namespace std;
int main(){
	int x1,x2,x3,sum,c1=0,c2=0,c3=0;
	char k1,k2,k3;
	cin>>k1>>k2>>k3;
	if(k1=='2')x1=2;
	if(k1=='3')x1=3;
	if(k1=='4')x1=4;
	if(k1=='5')x1=5;
	if(k1=='6')x1=6;
	if(k1=='7')x1=7;
	if(k1=='8')x1=8;
	if(k1=='9')x1=9;
	if(k1=='T'||k1=='J'||k1=='Q'||k1=='K')x1=10;
	if(k1=='A'){
		x1=11;
		c1=1;
	}
	if(k2=='2')x2=2;
	if(k2=='3')x2=3;
	if(k2=='4')x2=4;
	if(k2=='5')x2=5;
	if(k2=='6')x2=6;
	if(k2=='7')x2=7;
	if(k2=='8')x2=8;
	if(k2=='9')x2=9;
	if(k2=='T'||k2=='J'||k2=='Q'||k2=='K')x2=10;
	if(k2=='A'){
		x2=11;
		c2=1;
	}
	if(k3=='2')x3=2;
	if(k3=='3')x3=3;
	if(k3=='4')x3=4;
	if(k3=='5')x3=5;
	if(k3=='6')x3=6;
	if(k3=='7')x3=7;
	if(k3=='8')x3=8;
	if(k3=='9')x3=9;
	if(k3=='T'||k3=='J'||k3=='Q'||k3=='K')x3=10;
	if(k3=='A'){
		x3=11;
		c3=1;
	}
	sum=x1+x2+x3;
	if(c1!=1&&c2!=1&&c3!=1&&sum<=21)cout<<"score:"<<sum;
	else if(c1==1&&c2==1&&c3==1)cout<<"score:"<<sum-20;
	else if(c1==1&&c2==1&&c3!=1){
		if(sum<=31)cout<<"score:"<<sum-10;
	else cout<<"score:"<<sum-20;}
	else if(c1==1&&c2!=1&&c3==1){
		if(sum<=31)cout<<"score:"<<sum-10;
	else cout<<"score:"<<sum-20;}
	else if(c1!=1&&c2==1&&c3==1){
		if(sum<=31)cout<<"score:"<<sum-10;
	else cout<<"score:"<<sum-20;}
	else if(c1!=1&&c2!=1&&c3!=1&&sum>21)cout<<sum<<" busted!";
	else if(c1==1||c2==1||c3==1&&sum>21){
		if(sum<=31)cout<<"score:"<<sum-10;
		else cout<<"score:"<<sum-10<<"busted!";
	}
	else cout<<"score:"<<sum;
	return 0;
}

11.
#include<iostream>
using namespace std;
int main(){
float size_smaller,size_larger,price_smaller,price_larger;
cout<<"sizesmall?\n";
cin>>size_smaller;
size_smaller/=2;
cout<<"sizelarge?\n";
cin>>size_larger;
size_larger/=2;
cout<<"pricesmall?\n";
cin>>price_smaller;
cout<<"pricelarge?\n";
cin>>price_larger;
float a1=price_smaller/(size_smaller*size_smaller*3.14);
float a2=price_larger/(size_larger*size_larger*3.14);
if(a1>a2)
	cout<<"larger one is the best buy!\n";
else if(a1<a2)
	cout<<"smaller one is the best buy!\n";
else cout<<"same\n";
	return 0;
}

12.
#include<iostream>
using namespace std;
int main(){
double x,y,time;
cout<<"principal amount?\n";
cin>>x;
if(x<0)cout<<"error!\n";
cout<<"yrs?\n";
cin>>y;
if(y<0)cout<<"error!\n";
if(x<1000)cout<<"total:"<<x*(1.0+0.025*y);
if(x>=1000&&x<10000)cout<<"total:"<<x*(1.0+0.02*y);
if(x>=10000&&x<100000)cout<<"total:"<<x*(1.0+0.015*y);
if(x>=100000)cout<<"total:"<<x*(1.0+0.01*y);
	return 0;
}

14.
#include<iostream>
#include<string>
using namespace std;
int main(){
string a[]={"Ninety","Eighty","Seventy","Sixty","Fifty","Forty","Thirty","Twenty"};
string b[]={"Nine","Eight","Seven","Six","Five","Four","Three","Two","One",""};
string c[]={"Nineteen","Eighteen","Seventeen","Sixteen","Fifteen","Fourteen","Thirteen","Twelve","Eleven","Ten"};
int cout1=10;
int i,j;
for(i=0;i<8;i++){
	for(j=0;j<10;j++){
		if(j>0){
		cout<<a[i];
		cout<<b[j]<<" bottles of beer on the wall."<<endl<<endl;
		}
		cout<<a[i];
		cout<<b[j]<<" bottles of beer on the wall,"<<endl;
		cout<<a[i];
		cout<<b[j]<<" bottles of beer,\nTake one down, pass it around,"<<endl;
	}
}
for(i=0;i<10;i++){
	if(i>0){
		cout<<c[i]<<" bottles of beer on the wall."<<endl<<endl;
	}
	cout<<c[i]<<" bottles of beer on the wall,"<<endl;
	cout<<c[i]<<" bottles of beer,\nTake one down, pass it around,"<<endl;
}
for(i=0;i<9;i++){
	if(i>0){cout<<b[i]<<" bottles of beer on the wall."<<endl<<endl;}
			cout<<b[i]<<" bottles of beer on the wall,"<<endl;
			cout<<b[i]<<" bottles of beer,\nTake one down, pass it around,"<<endl;
			cout1--;
			if(cout1<=1)cout<<"Zero bottles of beer on the wall.";}

	return 0;
}
