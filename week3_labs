1.
#include<iostream>
using namespace std;
int main(){
	int a,b,c;
	cout<<"input a b c for ax^2+bx+c=0\n";
	cin>>a;
	cin>>b;
	cin>>c;
	if(b*b-a*c<0)cout<<"there is no solution\n";
	else if(b*b-a*c==0)cout<<"there is one solution\n";
	else cout<<"there are two solution\n";
	return 0;
}

2.
#include<iostream>
using namespace std;
int main(){
	int y1,m1,d1,y2,m2,d2,x;
	cout<<"input birthday of child1\n";
	cin>>y1>>m1>>d1;
	d1=y1*360+m1*30+d1;
	cout<<"input birthday of child2\n";
	cin>>y2>>m2>>d2;
	d2=y2*360+m2*30+d2;
	if(d1<d2)x=d2-d1;
	else x=d1-d2;
	y1=x/360;
	x=x%360;
	m1=x/30;
	x=x%30;
	cout<<"threre are "<<y1<<" yrs "<<m1<<" mons "<<x<<" days between child1 and 2\n";
	return 0;
}

3.
#include<iostream>
using namespace std;
int main(){
	int h,dp;
	double g;
	cout<<"hrs a week?\n";
	cin>>h;
	cout<<"dependents?\n";
	cin>>dp;
	g=(h-40)*16.78*1.5+h*16.78;
	if(h<= 40&&dp<3)cout<<"gross:"<<static_cast<double>(h)*16.78<<" net income:"<<static_cast<double>(h)*16.78*0.75-10<<endl;
	if(h<=40&&dp>=3)cout<<"gross:"<<static_cast<double>(h)*16.78<<" net income:"<<static_cast<double>(h)*16.78*0.75-45<<endl;
	if(h>40&&dp<3)cout<<"gross:"<<g<<" net income:"<<g*0.75-10<<endl;
	if(h>40&&dp>=3)cout<<"gross:"<<g<<" net income:"<<g*0.75-45<<endl;
	return 0;
}

4.
#include<iostream>
using namespace std;
int main(){
	int a[10];
	int ps=0,ns=0,cout1=0,cout2=0,n=0,i;
	cout<<"input 10 numbers\n";
	for(i=0;i<10;i++){
	cin>>a[i];
	}
	for(i=0;i<10;i++){
	if(a[i]<0){
		ns+=a[i];
		n+=a[i];
		cout2++;
	}
	else if(a[i]>0){
		ps+=a[i];
		n+=a[i];
		cout1++;
	}
	else n+=a[i];
	if (cout1==0) cout1=1;
	else if(cout2==0) cout2=1;
	else continue;
}
	cout<<"postive sum:"<<ps<<" postive avg:"<<static_cast<double>(ps)/cout1<<endl;
	cout<<"negetive sum:"<<ns<<" negetive avg:"<<static_cast<double>(ns)/cout2<<endl;
	cout<<"total sum:"<<n<<" total avg:"<<static_cast<double>(n)/10<<endl;
        return 0;
}

