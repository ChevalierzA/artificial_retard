void fun6(){
    int n;
    int j;
    cin>>n;
    for (int i=2; i<n+2; i++) {
        for(j=n;;j++){
            if (j%i==0) {
                continue;
            }
            else cout<<j;
        }
    }
    
}
void fun7(){
    for (int i=1; i<11; i++) {
        for(int j=0;j<i;j++)
            cout<<"*";
        cout<<endl;
    }
}
void fun8(){
    for (int i=1; i<11; i++) {
        for(int j=1;j<11;j++){
            if (j+i<11) {
                cout<<" ";
            }
            else cout<<"*";
            
        }
        
        cout<<endl;
    }
    
}
void fun9(){
    for (int i=1; i<=10; i++) {
        for(int j=1;j<=19;j++){
            if (i+j>10&&i+j<10+2*i) {
                cout<<"*";
            }
            else cout<<" ";
            
        }
        
        cout<<endl;
    }
    
}
void fun10(){
    for (int i=1; i<=10; i++) {
        for(int j=1;j<=20-i;j++){
            if (j<i) {
                cout<<" ";
            }
            else cout<<"*";
            
        }
        
        cout<<endl;
    }
}
void fun12(){
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=i;j++){
            cout<<i*j<<"\t";
        }
        cout<<endl;
    }
    
}
void fun13(){
    int n;
    cin>>n;
    for(int i=1;i<=2*n-1;i++){
        for(int j=1;j<=n;j++){
            if (i<=n) {
                if(i<j)cout<<"*";
            }
            else{
                if (i+j-n<n) {
                    cout<<"*";
                }
            }
        }
        cout<<endl;
    }
    void fun14(){
    int n;
    cin>>n;
    for(int i=1;i<=2*n-1;i++){
        for(int j=1;j<=n;j++){
            if (i<=n) {
                if(i<=j)cout<<" ";
                else cout<<"*";
            }
            else{
                if (i+j-n<=n) {
                    cout<<"*";
                }
                else cout<<" ";
            }
        }
        cout<<endl;
    }
}
