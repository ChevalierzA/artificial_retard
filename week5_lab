
#include <iostream>

using namespace std;
int main(){
    //fun1();

}

void fun1(){
    int a;
    int b;
    cin>>a;
    cin>>b;
    if (a>b) {
        int temp=a;
        a=b;
        b=temp;
    }
    a++;
    b--;
    for (int i=a; i<=b; i++) {
        cout<<i;
    }
}

