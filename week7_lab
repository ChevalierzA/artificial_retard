#include <iostream>
#include <cstdlib>
#include <time.h>
using namespace std;
float numGenerator(){
    srand(time(0));
    float a=rand();
    a=(float)(a)/RAND_MAX*2-1;
    return a;
}

bool isPrime(int x){
    int count=0;

    for (int i=1; i<x; i++) {
        if(x%i==0)
            count++;
    }
    if (count>2) {
        return 0;
    }
    else return 1;
}
int sizeCounter(char*a){
    int count = 0;
    for (int i=0; a[i]!='\0'; i++) {
        if(a[i]=='a')count++;
        if(a[i]=='e')count++;
        if(a[i]=='i')count++;
        if(a[i]=='o')count++;
        if(a[i]=='u')count++;
        if(a[i]=='A')count++;
        if(a[i]=='E')count++;
        if(a[i]=='I')count++;
        if(a[i]=='O')count++;
        if(a[i]=='U')count++;
    }
    return count;
}
