#include <iostream>
#include<string>
using namespace std;
void testfield(){
	int a = 8;
	int *b = &a;
	int d = a+*b;
	cout<<d<<endl;
	*b = 10;
	cout<<*b<<endl;
	cout<<a<<endl;
	*(b+1)=10;
	cout<<*(b+1);
}
void foo(int &m, int n, int *x, int *y)
{
x = y;
*y = 7;
m = 8;
n = *x;
cout << m << ", " << n << ", " << *x << ", " << *y << endl;
return;
}

int main(){/*
