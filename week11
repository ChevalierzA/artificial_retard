#include<iostream>
#include<cstdlib>
using namespace std;
void f1(){
int*x1=new int[10];
float*x2=new float[10];
double*x3=new double[10];
char*x4=new char[10];
bool*x5=new bool[10];

for(int i=0;i<10;i++){
x1[i]=rand()%11-5;
x2[i]=1.0*rand()/RAND_MAX;
x3[i]=2.0*rand()/RAND_MAX;
x4[i]=rand()%26+97;
x5[i]=rand()%2==0?true:false;
}
cout<<x1<<endl;
cout<<x2<<endl;
cout<<x3<<endl;
cout<<x4<<endl;//<<output stream (function)
cout<<x5<<endl;
delete[] x1;delete[] x2;delete[] x3;delete[] x4;delete[] x5;
}

void f2(){
int x1[10];
float x2[10];
double x3[10];
char x4[10];
bool x5[10];

for(int i=0;i<10;i++){
x1[i]=rand()%11-5;
x2[i]=1.0*rand()/RAND_MAX;
x3[i]=2.0*rand()/RAND_MAX;
x4[i]=rand()%26+97;
x5[i]=rand()%2==0?true:false;
}
x4[9]='\0';
cout<<x1<<endl;
cout<<x2<<endl;
cout<<x3<<endl;
cout<<x4<<endl;
cout<<x5<<endl;
}
void f3(){
	char A[]={'f','u','c','k','\0'};
	for(int i=0;A[i]!='\0';i++){
	cout<<A[i];
	}
	cout<<endl;
	cout<<A;
}
void f4(){
	char A[]={'f','u','\0','k','\0'};
	for(int i=0;A[i]!='\0';i++){
	cout<<A[i];
	}
	cout<<endl;
	cout<<A;
}
int strlength(char*A){
	int index = 0;
	while(A[index]!='\0')index++;
	return index;
}
bool isFound(char*A,char x){
	for(int i=0;A[i]!='\0';i++){
	if(A[i]==x) return true;
	}
	return false;
}
void f5(){
	char A[]="dick";
	int len = strlength(A);
	char x;
	cin>>x;
	bool flag = isFound(A,x);
	if(flag)cout<<"true"<<endl;
	else cout<<"false"<<endl;

}
