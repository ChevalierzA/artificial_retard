int main()
{
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
		for(int j=1;j<=n;j++){
			if(i+j<n+1) cout<<"\t";
			else
		cout<<i*(i+j-n)<<"\t";
		}
		cout<<endl;
	}
}
