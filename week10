#include<iostream>
#include<cmath>
#include<ctime>
using namespace std;
int main()
{
	
	srand(time(0));
	int size;
	cin>>size;
	int *A;
	A = new int[size];
	for(int i = 0;i<size;i++){
	A[i] = 10-i;
	}
	for(int i = 0;i<size;i++){
	cout<<A[i]<<endl;
	}
	for(int i = 0;i<size;i++){
	A[i] = 2*A[i];
	}
	for(int i = 0;i<size;i++){
	cout<<A[i]<<endl;
	}
	delete[] A;//To delete all elements in heap

}
