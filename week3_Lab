//q15
int main()
{
	int n;
	bool isPrime=1;
	for(int i=1;i<=n;i++){
		cin>>n;
	if(n!=2&&n%2==0) isPrime=0;
	if(n==2)isPrime=1;
	}
	if(isPrime)
		cout<<"Not Composite\n";
	else
		cout<<"Composite\n";
return 0;
}
