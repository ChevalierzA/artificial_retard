	{
		if(flag<0)
		{
			if((0-flag)<(0-min))
				flag=min;	
	}
	}
	if(min>0)
	{
		if(flag>0)
		{
		if(flag<min)
			flag=min;
		}
		else if(0-flag<min)
			flag=min;
	}
}
cout << " The maximum in absolute value is " << flag << endl;
return 0;
}
